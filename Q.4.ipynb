   "source": [
    "\"\"\"Check many times a given number can be divided by 3 before it is less than or equal to 10.\"\"\"\n",
    "num=int(input(\"enter any number:\"))\n",
    "i=0\n",
    "n=num\n",
    "while n!=0:\n",
    "   n=num//3\n",
    "   num=n\n",
    "   i=i+1\n",
    "   if num<=10:\n",
    "    break\n",
    "print(i,\"times divided by 3\")"
   ]
